{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",
        "almasaeed2010/adminlte": "^3.2",
        "barryvdh/laravel-elfinder": "0.5.3",
        "guzzlehttp/guzzle": "*",
        "laravel-notification-channels/telegram": "^5.0",
        "laravel/framework": "^10",
        "laravel/tinker": "*",
        "spatie/laravel-permission": "*"
    },
    "require-dev": {
        "appzcoder/crud-generator": "^4.0",
        "barryvdh/laravel-debugbar": "^3.14",
        "barryvdh/laravel-ide-helper": "^3.1",
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^1.29",
        "laravel/pint": "*",
        "laravel/sail": "^1.34",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.10",
        "phpunit/phpunit": "^10.5",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "rm -fv public/vendor/dist || true && mkdir -p public/vendor || true && ln -s -r ./vendor/almasaeed2010/adminlte/dist public/vendor/dist",
            "ln -s -r ./vendor/almasaeed2010/adminlte/plugins public/vendor/plugins || true",
            "ln -s -r ./vendor/studio-42/elfinder public/vendor/elfinder-2.1.62 || true"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
